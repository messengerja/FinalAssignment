<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:FinalAssignment.Styles">
    
    <!-- Typographical Styles -->
    <Style x:Key="TitleText" TargetType="{x:Type TextBlock}">
        <Setter Property="FontFamily" Value="MS UI Gothic" />
        <Setter Property="TextAlignment" Value="Center" />
        <Setter Property="FontSize" Value="Center" />
        <Setter Property="Margin" Value="10" />
    </Style>

    <!-- Theme -->
    <SolidColorBrush Color="BurlyWood" x:Key="PrimaryColor" />
    <SolidColorBrush Color="#FFC8D5B9" x:Key="HighlightColor" />
    <SolidColorBrush Color="#FFC8D5B9" x:Key="ConstrastColor" />
    <SolidColorBrush Color="#FFC8D5B9" x:Key="AccentColor" />
    <SolidColorBrush Color="#FFC8D5B9" x:Key="DarkAccentColor" />
    

</ResourceDictionary>